{
  "name": "alexsabur/orchid-editorjs-field",
  "description": "A orchid editorjs field",
  "license": "MIT",
  "keywords": [
    "orchid",
    "editorjs",
    "field"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Alex",
      "email": "alexsabur@live.ru"
    }
  ],
  "require": {
    "php": ">=7.2",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/support": "^6.0|^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "AlexSabur\\OrchidEditorJSField\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlexSabur\\OrchidEditorJSField\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "AlexSabur\\OrchidEditorJSField\\Providers\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
